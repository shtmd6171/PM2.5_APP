    <TextView android:id="@+id/t4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="23sp" android:layout_marginTop="56dp" android:layout_marginBottom="10dp" android:text="초미세먼지 : " android:layout_below="@+id/item_location1" android:layout_marginStart="10dp"/>

    <ImageView android:id="@+id/star1" android:layout_width="35dp" android:layout_height="35dp" android:layout_centerVertical="true" android:layout_marginRight="15dp" android:layout_alignParentRight="true" android:src="@drawable/star" android:scaleType="fitXY" android:adjustViewBounds="true"/>

</RelativeLayout>


